{
  "name": "steal-qunit",
	"main": "steal-qunit.js",
  "version": "0.1.1",
  "dependencies": {
    "qunit": "~1.14.0"
  },
  "ignore": [
    "**/.*",
    "package.json",
    "bower.json",
    "Gruntfile.js",
    "node_modules",
    "test"
  ],
	"system": {
		"paths": {
			"qunit/qunit/*": "bower_components/qunit/qunit/*.js",
			"qunit/qunit/qunit.css": "bower_components/qunit/qunit/qunit.css"
		},
		"meta": {
			 "qunit/qunit/qunit": {
					"format": "global",
					"exports": "QUnit",
					"deps": [
						"steal-qunit/add-dom"
					]
				}
		}
	}
}

{
  "name": "steal-qunit",
  "version": "0.0.1",
  "description": "A bower package for QUnit and StealJS",
  "main": "steal-qunit",
  "system": {
    "npmIgnore": {
      "bower": true,
	  "grunt": true,
	  "grunt-cli": true,
	  "grunt-contrib-copy": true,
	  "grunt-contrib-watch": true,
	  "steal": true,
	  "steal-tools": true
    },
    "meta": {
      "./qunit": {"format": "global", "exports": "QUnit", "deps": ["./add-dom"]}
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitovi/steal-qunit.git"
  },
  "keywords": [
    "StealJS",
    "QUnit"
  ],
  "author": "Bitovi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitovi/steal-qunit/issues"
  },
  "homepage": "https://github.com/bitovi/steal-qunit",
  "devDependencies": {
    "steal": "bitovi/steal#npm",
    "steal-tools": "bitovi/steal-tools#npm",
    "bower": "1.3.8",
    "grunt": "~0.4.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-watch": "~0.3.0"
  }
}
